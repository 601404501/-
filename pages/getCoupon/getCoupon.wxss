
.no-order{
  color: #CCC;
  font-size: 28rpx;
  text-align: center;
  padding-top: 430rpx;
}
.no-order .icon-no-data{
  font-size: 160rpx;
  
  margin-bottom: 28rpx;
}
.list-item{
  width: 690rpx;
  height: 240rpx;
  margin: 0 auto;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-top: 20rpx; 
}
.coupon-content{
  float: left;
  width: 240rpx;
  text-align: center;
  color: #FFF;
  font-size: 24rpx;
}
.coupon-info{
  margin-left: 260rpx;
  padding-left: 20rpx;
  padding-top: 30rpx;
}
.coupon-amount{
  font-size: 50rpx;
  padding-top: 40rpx;
}
.coupon-amount text{
  font-size: 90rpx;
}
.coupon-name{
  font-size: 32rpx;
  color: #000;
  margin-bottom: 25rpx;
}
.coupon-time{
  color: #999;
  font-size: 24rpx;
  margin-bottom: 25rpx;
}
.coupon-btn{
  display: inline-block;
  width: 154rpx;
  height: 50rpx;
  line-height: 50rpx;
  border: 1rpx solid #FF7474;
  color: #FF7474;
  text-align: center;
  border-radius: 25rpx;
  font-size: 24rpx;
}

.list-item{
  background-image: url(data:image/png;base64,);
}