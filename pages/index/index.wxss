.pull-right{
  position: absolute;
  right: 30rpx;
}
.section-title .pull-right{
  font-size: 26rpx;
  color: #999;
}
.shop-info{
  background-color: #FFF;
  font-size: 24rpx;
  color: #666;
  padding: 20rpx 30rpx 0 30rpx;
  line-height: 50rpx;
  border: 1px solid #EEE;
  border-radius: 10rpx;
  width: 710rpx;
  box-sizing: border-box;
  margin: 30rpx auto;
  box-shadow: 0 0 14rpx #CCC;
}
.shop-info-item{
  position: relative;
}
.shop-info-item .icon-phone{
  color: #FF6161;
}
.shop-info-item .icon-navigate{
  color: #4278F2;
}
.shop-info-item:last-child{
  padding-bottom: 20rpx;
}
.title{
  margin: 0 auto;
  text-align: center;
  vertical-align: middle;
  padding: 45rpx 0;
  background-color: #FFF;
}
.title .line{
  display: inline-block;
  height: 1rpx;
  width: 180rpx;
  background-color: #CCC;
  vertical-align: middle;
}
.title .txt{
  display: inline-block;
  vertical-align: middle;
  margin: 0 60rpx;
}
swiper{
  height: 400rpx;
  width: 100%;
  margin: 0 auto;
  background-color: #FFF;
}
swiper-item image{
  display: block;
  width: 100%;
  height: 100%;
}
.navbar{
  background-color: #FFF;
  padding: 50rpx 0 30rpx 0;
}
.navbar::after{
  display: block;
  content: ' ';
  clear: both;
}
.navbar-item{
  width: 25%;
  float: left;
  text-align: center;
  margin-bottom: 20rpx;
}
.navbar-icon{
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
}
.navbar-icon image{
  display: block;
  width: 100%;
  height: 100%;
}
.navbar-txt{
  font-size: 28rpx;
}
.section-title{
  height: 100rpx;
  width: 100%;
  text-align: center;
  line-height: 100rpx;
  position: relative;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;  
}
.section-title{
  display: block;
  width: 100%;
  height: 100%;
  z-index: 1;
  left: 0;
  top: 0;
  position: relative;
}
.section-title text{
  z-index: 1500;
}
.list{
  margin-bottom: 20rpx;
}
.list-item{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 30rpx;
  background-color: #FFF;
  margin-top: 20rpx;
}
.list-pic{
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
}
.list-pic image{
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.list-name{
  font-size: 32rpx;
  color: #000;
  margin-bottom: 20rpx;
}
.list-post{
  margin-bottom: 20rpx;
}
.list-result{
  font-size: 26rpx;
  color: #999;
}
.list-content{
  padding-left: 20rpx;
  width: 357rpx;
  box-sizing: border-box;
}
.list-btn text{
  padding: 19rpx 27rpx;
  color: #FFF;
  background-color: #FF86A5;
  border-radius: 10rpx;
  font-size: 28rpx;
}
.grids{
  padding: 20rpx 0 0 20rpx;
}
.grids::after{
  display: block;
  content: " ";
  clear: both;
}
.grids-item{
  float: left;
  width: 345rpx;
  background-color: #FFF;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.grids-pic{
  width: 345rpx;
  height: 345rpx;
}
.grids-pic image{
  display: block;
  width: 100%;
  height: 100%;
}
.grids-name{
  color: #000;
}
.grids-price{
  color: #FF6589;
  font-size: 32rpx;
}
.grids-info{
  border-left: 3px solid #FF6589;
  padding-left: 20rpx;
}
.grids-content{
  padding: 20rpx 0;
}