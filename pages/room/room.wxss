page{
  background-color: #F8F8F8;
}
.room-pic{
  width: 320rpx;
  height: 232rpx;
  float: left;
}
.room-pic image{
  display: block;
  width: 100%;
  height: 100%;
}
.room-info{
  margin-left: 350rpx;
  height: 232rpx;
  padding: 20rpx 0;
  box-sizing: border-box;
}
.list-item{
  padding: 30rpx;
  background-color: #FFF;
  margin-bottom: 20rpx;
}
.room-name{
  font-size: 32rpx;
  color: #000;
  margin-bottom: 20rpx;
}
.room-nums{
  font-size: 26rpx;
  color: #999;
  margin-bottom: 20rpx;
}
.room-btn{
  padding: 19rpx 27rpx;
  font-size: 28rpx;
  color: #FFF;
  background-color: #FF86A5;
  border-radius: 10rpx;
  display: inline-block;
}